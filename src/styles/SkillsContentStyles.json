{
    "glass":{
        "background": "rgba( 255, 255, 255, 0.1)",
        "boxShadow": "0 0 10px  #0d6efd",
        "backdropFilter": "blur( 2px )",
        "borderRadius": "10px",
        "border": "1px solid rgba( 255, 255, 255, 0.18 )",
        "maxWidth":"70vw",
        "minHeight":"50%",
        "display":"flex",
        "flexDirection":"column",
        "justify-content":"flex-start",
        "align-items":"center",
        "padding":"20px",
        "skillsContainer":{
            "display":"flex",
            "justifyContent":"center",
            "flexWrap":"wrap",
            "padding":"0 10px",
            "name":{
                "color":"white",
                "padding":"5px 10px",
                "borderRadius":"5px",
                "margin":"5px",
                "background":"rgba( 255, 255, 255, 0.3)",
                "backdropFilter": "blur( 2px )"
            },
            
            "width":"100%"
        }
    },
    "text":{
        "title":{
            "fontSize":"40px",
            "fontWeight":"bold",
            "color":"white",
            "marginBottom":"10px",
