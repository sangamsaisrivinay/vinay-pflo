{
    "schoolsContainer":{
        "display":"flex",
        "flexDirection":"column",
        "alignItems":"center",
        "justifyContent":"space-around",
        "width":"100%",
        "height":"auto",
        "background":"#f2efea",
        "padding":"20px 0"
    },
    "title":{
        "width":"100%",
        "textTransform":"capitalize",
        "fontSize":"2.5rem",
        "fontWeight":"bold",
        "marginBottom":"20px",
        "textAlign":"center"
    },
    "card":{
        "border":"1px solid #ccc",
        "borderRadius":"10px",
        "boxShadow":"2px 2px 6px 0px rgba(0,0,0,0.3)",
        "width":"80%",
        "marginBottom":"20px",
        "padding":"20px",
        "display":"flex",
        "flexWrap":"wrap",
        "flexDirection":"column",
        "alignItems":"center",
        "justifyContent":"space-around",
        "background":"#fff",
        "lineOne":{
            "display":"flex",
            "flexWrap":"wrap",
            "flexDirection":"row",
            "alignItems":"center",
            "justifyContent":"space-between",
            "width":"100%",
            "marginBottom":"10px",
            "schoolName":{
                "fontSize":"1.5rem",
                "fontWeight":"bold",
                "textTransform":"capitalize"
            },
            "year":{
                "fontSize":"1.3rem",
                "color":"white",
                "fontWeight":"bold",
                "background":"gray",
                "padding":"5px",
                "borderRadius":"5px"
            }
        },
        "lineTwo":{
            "display":"flex",
            "flexWrap":"wrap",
            "flexDirection":"row",
            "alignItems":"center",
            "justifyContent":"flex-start",
            "width":"100%",
            "marginBottom":"5px",
            "course":{
                "fontSize":"1.1rem",
                "marginRight":"5px",
                "fontWeight":"bold"
            },
            "branch":{
                "color":"gray",
                "fontStyle":"italic"
            }
        },
        "lineThree":{
            "width":"100%",
            "display":"flex",
            "flexWrap":"wrap",
            "flexDirection":"row",
            "justifyContent":"space-between"
        }
        
    }
}